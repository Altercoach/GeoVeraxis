{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18"
  },
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "httpsOnly": true,
    "redirects": [
      {
        "source": "/test/**",
        "destination": "https://staging.geoveraxis.com",
        "type": 302
      },
      {
        "source": "/**",
        "destination": "https://www.geoveraxis.com",
        "type": 301
      }
    ]
  }
}
